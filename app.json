{
  "expo": {
    "name": "myvideocallingapp",
    "slug": "myvideocallingapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["CAMERA", "RECORD_AUDIO"],
      "package": "com.subbu_dhoni.myvideocallingapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5aaa0505-7dc4-4404-a6af-efe111fcb79a"
      }
    },
    "plugins": [
      [
        "react-native-twilio-video-webrtc",
        {
          "cameraPermission": "Allow VideoCallApp to access your camera",
          "microphonePermission": "Allow VideoCallApp to access your microphone"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.0"
          },
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          }
        }
      ]
    ]
  }
}